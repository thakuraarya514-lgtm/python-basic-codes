num = int(input("Enter a number: "))

if num % 2 == 0:
    print("Even Number")
else:
    print("Odd Number")
a = 10
b = 25
c = 7

largest = max(a, b, c)
print("Largest:", largest)

numbers = [10, 20, 30, 40]
total = sum(numbers)
print("Sum =", total)


text = input("Enter text: ").lower()
vowels = "aeiou"

count = sum(1 for char in text if char in vowels)
print("Total vowels:", count)
